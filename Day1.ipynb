    "def fy_shuffle(a):\n",
    "    ### your code here ###\n",
    "    n = len(a)\n",
    "    b = []\n",
    "    \n",
    "    while n > 1:\n",
    "        idx = randint(0, n-1)\n",
    "        swap = a.pop(idx)\n",
    "        b.append(swap)\n",
    "        n -=1\n",
    "    b.append(a.pop(0))\n",
    "    \n",
    "    return b\n",
    "    \n",
    "# testing\n",
    "seed(69)\n",
    "fy_shuffle(list(range(1,11))) # shuffle the numbers 1 to 10"
   ]
